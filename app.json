{
  "expo": {
    "name": "NepalDisasterManagement",
    "slug": "NepalDisasterManagement",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nepaldisastermanagement",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anikesh1.NepalDisasterManagement",
      "navigationBar": {
        "visible": "immersive",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 160,
          "imageHeight": 160,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-system-ui",
      "expo-navigation-bar"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d115e6e8-100f-4e40-aad7-aea97086f74f"
      }
    }
  }
}
